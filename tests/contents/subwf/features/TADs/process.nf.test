nextflow_process {

    name "Test Process HiCExplorerFindTADs"
    script "contents/subwf/features/TADs/process.nf"
    process "TADS"

    test("SmokeTest_ProcessTADS") {
        tag "10to60s"
        when {
            process {
                """
                input[0] = tuple(
                    "test", 
                    "$projectDir/tests/assets/downloads/mcool/full/GSM4604274_629.iced.mcool", 
                    [resolution: 100000],
                    "--minDepth 400000 --maxDepth 1000000 --step 100000 --chromosomes chr19 --correctForMultipleTesting bonferroni"
                )
                """
            }
        }

        then {
            assert process.success
        }

    }

}
